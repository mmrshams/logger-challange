{
  "name": "log-parser",
  "version": "1.0.0",
  "description": "Genral log-parser with typescript",
  "main": "parser.ts",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "@types/jest": "^27.0.3",
    "jest": "^27.4.3",
    "ts-jest": "^27.1.1",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.1",
    "@types/node": "^15.12.4",
    "@types/uuid": "^8.3.0",
    "dotenv": "^10.0.0",
    "gts": "^3.1.0",
    "nodemon": "^2.0.7",
    "standard-version": "^9.3.1",
    "typescript": "^4.3.4"
  },
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node --require dotenv/config dist/src/main.js --input ./app.log --output ./errors.json --level error",
    "test": "jest --config ./jest-config.json",
    "watch": "nodemon --ext js,json,ts --watch ./src --exec \"yarn start\"",
    "lint": "gts lint",
    "fix": "gts fix",
    "clean": "gts clean"
  },
  "author": "omid shams",
  "license": "ISC"
}
